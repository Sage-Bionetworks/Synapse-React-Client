{
  "steps": [
    {
      "id": "naming",
      "order": 5,
      "title": "Naming",
      "default": "measurements",
      "child": true,
      "validationRules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "naming",
                "operator": "equal",
                "path": ".first_name",
                "value": "bob"
              }
            ]
          },
          "event": {
            "type": "validation",
            "params": {
              "message": "is bad",
              "name": "bad",
              "property": ".naming.first_name"
            }
          },
          "priority:": 9
        },
        {
          "conditions": {
            "all": [
              {
                "fact": "naming",
                "operator": "notEqual",
                "path": ".first_name",
                "value": "jane"
              },
              {
                "fact": "naming",
                "operator": "equal",
                "path": ".last_name",
                "value": "doe"
              }
            ]
          },
          "event": {
            "type": "validation",
            "params": {
              "message": "is bad2 last",
              "name": "bad",
              "property": ".naming.last_name"
