{
  "definitions": {
    "contribution_field": {
      "title": "",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": ["data"],
          "title": "Data"
        },
        {
          "type": "string",
          "enum": ["analysis"],
          "title": "Analysis"
        },
        {
          "type": "string",
          "enum": ["tools"],
          "title": "Tools"
        }
      ]
    },
    "data_origin_field": {
      "title": "",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": ["human"],
          "title": "Human"
        },
        {
          "type": "string",
          "enum": ["model_system"],
          "title": "Model System"
        }
      ]
    },
    "model_system_field": {
      "title": "",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": ["mouse"],
          "title": "Mouse"
        },
        {
          "type": "string",
          "enum": ["rat"],
          "title": "Rat"
        },
        {
          "type": "string",
          "enum": ["drosophila"],
          "title": "Drosophila"
        },
        {
          "type": "string",
          "enum": ["cell_culture"],
          "title": "Cell Culture"
        },
        {
          "type": "string",
          "enum": ["other"],
          "title": "Other"
        }
      ]
    },
    "assays_field": {
      "type": "object",
      "title": "Assays",
      "properties": {
        "assays_list": {
          "title": "List all assays specimens have been used for",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": ["RNAseq", "ChIPseq", "Other"]
          }
        },
        "assays_list_other": {
          "type": "string",
          "title": "If other - describe"
        }
      }
    },
    "assesments_field": {
      "type": "object",
      "title": "Assessments",
      "properties": {
        "assesments_list": {
          "type": "array",
          "title": "List all assessments",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": ["Cognition", "Behavioral", "Other"]
          }
        },
        "assesments_list_other": {
          "type": "string",
          "title": "If other - describe"
        }
      }
    },
    "study": {
      "type": "object",
      "required": ["submission_name", "content_summary"],
      "properties": {
        "submission_name": {
          "title": "Submission Name",
          "type": "string"
        },
        "content_summary": {
          "title": "Summarize the content that will be contributed",
          "type": "string"
        }
      }
    },
    "contact": {
      "type": "object",
      "properties": {
        "name": {
          "title": "What is your name",
          "type": "string"
        },
        "email": {
          "title": "What is your email",
          "type": "string",
          "format": "email"
        },
        "institution": {
          "type": "string"
        }
      }
    },
    "funding": {
      "type": "object",
      "properties": {
        "nih_grant_number": {
          "title": "NIH grant number if applicable",
          "type": "string"
        },
        "nia_program": {
          "type": "string",
          "enum": ["AMP-AD", "M2OVE-AD", "MODEL-AD", "Resillience-AD", "Other"],
          "title": "NIA program if applicable"
        }
      },
      "dependencies": {
        "nia_program": {
          "oneOf": [
            {
              "properties": {
                "nia_program": {
                  "enum": ["AMP-AD", "M2OVE-AD", "MODEL-AD", "Resillience-AD"]
                }
              }
            },
            {
              "properties": {
                "nia_program": {
                  "enum": ["Other"]
                },
                "nia_program_other": {
                  "title": "If other - describe",
                  "type": "string"
                }
              }
            }
          ]
        }
      }
    },
    "contributions": {
      "type": "object",
      "required": ["contribution_type"],
      "properties": {
        "contribution_type": {
          "title": "What type of content are you contributing?",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/contribution_field"
          }
        },
        "contribution_size": {
          "type": "string",
          "title": " Estimated contribution size in GB",
          "enum": ["<1 GB", "1-100 GB", "100-1000 GB", ">1000 GB"]
        }
      }
    },
    "data": {
      "type": "object",
      "properties": {
        "data_origin": {
          "title": "What is the data origin?",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/data_origin_field"
          }
        }
      }
    },
    "human": {
      "type": "object",
      "properties": {
        "release_allowed_under_AMP-AD_KP": {
          "type": "boolean",
          "title": "Can data be released under the AMP-AD KP terms of use?",
          "default": false,
          "enum": [true, false],
          "enumNames": ["Yes", "No"],
          "uniqueItems": true
        },
        "study_type": {
          "type": "string",
          "title": "Study type",
          "enum": ["Prospective Cohort", "Case-control", "Post Mortem", "Other"]
        },
        "specimen_repository": {
          "title": "If post mortem - list brain bank or other specimen repository",
          "type": "string"
        },
        "disease_focus": {
          "type": "string",
          "title": "Disease focus",
          "enum": ["AD", "MCI", "Other"]
        },
        "specimen_source": {
          "type": "string",
          "title": "Specimen source",
          "enum": ["Serum", "Brain", "Other"]
        },
        "total_number_individuals": {
          "title": "Total number of individuals",
          "type": "number"
        },
        "total_number_specimens": {
          "title": "Total number of specimens",
          "type": "number"
        },
        "assays": {
          "$ref": "#/definitions/assays_field"
        },
        "assesments": {
          "$ref": "#/definitions/assesments_field"
        }
      },
      "dependencies": {
        "release_allowed_under_AMP-AD_KP": {
          "oneOf": [
            {
              "properties": {
                "release_allowed_under_AMP-AD_KP": {
                  "enum": [true]
                }
              }
            },
            {
              "properties": {
                "release_allowed_under_AMP-AD_KP": {
                  "enum": [false]
                },
                "release_not_allowed_explanation": {
                  "type": "string",
                  "title": "If no - describe"
                }
              }
            }
          ]
        },
        "study_type": {
          "oneOf": [
            {
              "properties": {
                "study_type": {
                  "enum": ["Prospective Cohort", "Case-control", "Post Mortem"]
                }
              }
            },
            {
              "properties": {
                "study_type": {
                  "enum": ["Other"]
                },
                "study_type_other": {
                  "title": "If other - describe",
                  "type": "string"
                }
              }
            }
          ]
        },
        "disease_focus": {
          "oneOf": [
            {
              "properties": {
                "disease_focus": {
                  "enum": ["AD", "MCI"]
                }
              }
            },
            {
              "properties": {
                "disease_focus": {
                  "enum": ["Other"]
                },
                "disease_focus_other": {
                  "title": "If other - describe",
                  "type": "string"
                }
              }
            }
          ]
        },
        "specimen_source": {
          "oneOf": [
            {
              "properties": {
                "specimen_source": {
                  "enum": ["Serum", "Brain"]
                }
              }
            },
            {
              "properties": {
                "specimen_source": {
                  "enum": ["Other"]
                },
                "specimen_source_other": {
                  "type": "string"
                }
              }
            }
          ]
        }
      }
    },
    "model_system": {
      "type": "object",
      "properties": {
        "model_system_type": {
          "type": "array",
          "title": "List the model systems used",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enumNames": [
              "Mouse",
              "Rat",
              "Drosophila",
              "Cell Culture",
              "Other"
            ],
            "enum": ["mouse", "rat", "drosophila", "cell_culture", "other"]
          }
        },
        "model_system_type_other": {
          "title": " If other - describe",
          "type": "string"
