{
  "name": "synapse-react-client",
  "version": "0.1.7",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sage-Bionetworks/Synapse-React-Client"
  },
  "dependencies": {
    "markdown-it": "^8.4.2",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emphasis-alt": "^1.0.8",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.1",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.4",
    "markdown-it-synapse-table": "^1.0.6",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "plotly.js": "^1.39.4",
    "prop-types": "^15.6.2",
    "react-plotly.js": "^2.2.0",
    "sanitize-html": "^1.18.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "NODE_PATH=./src react-scripts start",
    "start": "NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build-js": "NODE_PATH=./src rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build": "NODE_PATH=./src npm-run-all build-css build-js",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject"
  }
}
