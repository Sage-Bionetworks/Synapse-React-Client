    "yarn": "1.15.2"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "prepublishOnly": "yarn run build && yarn run test:coverage",
    "predeploy": "yarn run build-gh && cp build/index.html build/404.html",
    "build-gh": "react-scripts-ts build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "yarn run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts-ts test --env=jsdom  --setupFiles ./src/setupTests.js ",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts-ts eject",
    "type-check": "tsc --noEmit",
    "tslintfix": "./node_modules/tslint/bin/tslint --project tsconfig.json --fix --",
    "type-check:watch": "yarn run type-check --watch",
    "build": "yarn run build-css && yarn run build:js && yarn run build:copy-assets && yarn build:rollup",
    "build:js": "rm -rf dist && tsc --p tsconfig.build.json",
    "build:copy-assets": "cd src/lib && find . -name '*.css' -o -name '*.svg' | cpio -pdm ../../dist/",
    "build:rollup": "rollup -c && cp -r src/umd dist/umd"
  },
  "resolutions": {
    "fstream": "1.0.12",
    "js-yaml": "3.13.1"
  }
}
